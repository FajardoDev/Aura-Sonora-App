{
  "expo": {
    "name": "Aura Sonora",
    "slug": "aura-sonora-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AuraSonoraApp.png",
    "scheme": "aurasonoraapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.fajardodevs.aurasonora",
      "supportsTablet": true
    },
    "android": {
      "package": "com.fajardodevs.aurasonora",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/AuraSonoraApp.png",
				"backgroundImage": "./assets/images/AuraSonoraApp.png",
				"monochromeImage": "./assets/images/logo1.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "userInterfaceStyle": "automatic",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      // "statusBar": {
      //   "translucent": true,
      //   "backgroundColor": "#00000000"
      // },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-audio",
			"expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "33d57962-13be-4d8e-9c89-997d4baaaf46"
      }
    },
    "owner": "fajardodevs"
  }
}
